   "source": [
    "def gcd(a, b):\n",
    "  while b != 0:\n",
    "    a, b = b, a % b\n",
    "  return a\n",
    "    #Пример\n",
    "num1 = 78\n",
    "num2 = 34\n",
    "result = gcd(num1, num2)\n",
    "print(f\"НОД чисел {num1} и {num2} равен {result}\")"
   ]
